{
  "name": "longport",
  "version": "3.0.4",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "longport",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "license": "MIT OR Apache-2.0",
  "description": "A Node.js library for LongPort Open API",
  "repository": {
    "type": "git",
    "url": "https://github.com/longportapp/openapi"
  },
  "homepage": "https://open.longportapp.com/en/",
  "devDependencies": {
    "@napi-rs/cli": "^2.9.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm --skip-gh-release",
    "version": "napi version"
  },
  "optionalDependencies": {
    "longport-win32-x64-msvc": "3.0.4",
    "longport-darwin-x64": "3.0.4",
    "longport-linux-x64-gnu": "3.0.4",
    "longport-darwin-arm64": "3.0.4",
    "longport-linux-arm64-gnu": "3.0.4",
    "longport-linux-x64-musl": "3.0.4"
  }
}